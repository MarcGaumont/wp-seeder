{
  "name": "jedrzejchalubek/wp-seeder",
  "description": "Seeds your WordPress database with test data.",
  "keywords": ["seed", "seeding", "wordpress"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jędrzej Chałubek",
      "email": "jedrzej.chalubek@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "fzaninotto/faker": "~1.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "brain/monkey": "~2.0",
    "satooshi/php-coveralls": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
